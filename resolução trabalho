//Faça um programa que imprima o produto entre dois números.
#include<stdio.h>
 int main() 
 {     
       float num1, num2, mult;     
       printf("Digite o primeiro numero: " );     
       scanf("%f", &num1);     
       printf("Digite o segundo numero: " );     
       scanf("%f", &num2);        
        /*Multiplicaçao    
        mult = num1 * num2;*/       
       printf("%.2f * %.2f = %.2f\n", num1, num2, mult);
       system("pause");       
       return (0); }
